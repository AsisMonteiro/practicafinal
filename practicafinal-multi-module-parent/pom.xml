			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<!-- Java 11 -->
					<release>11</release>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<modules>
		<module>practicafinal-multi-module-child-springrest</module>
		<module>practicafinal-multi-module-child-springdata</module>
		<module>practicafinal-multi-module-child-springmvc</module>
		<module>practicafinal-multi-module-child-negocio</module>
	</modules>
</project>